{
  "name": "@seedvault/server",
  "version": "0.2.0",
  "type": "module",
  "bin": {
    "seedvault-server": "bin/seedvault-server.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outfile dist/server.js --target bun && npx vite build ui",
    "dev": "DATA_DIR=./data bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "check": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collaborator-ai/seedvault.git",
    "directory": "server"
  },
  "dependencies": {
    "@seedvault/sdk": "workspace:*",
    "@seedvault/ui": "workspace:*",
    "hono": "^4",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "typescript": "^5",
    "vite": "7.3.1"
  }
}
