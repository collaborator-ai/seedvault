{
	"name": "@seedvault/cli",
	"version": "0.7.0",
	"type": "module",
	"bin": {
		"sv": "bin/sv.mjs"
	},
	"exports": {
		".": {
			"types": "./dist/api/index.d.ts",
			"import": "./dist/api/index.js",
			"default": "./dist/api/index.js"
		},
		"./client": {
			"types": "./dist/api/client.d.ts",
			"import": "./dist/api/client.js",
			"default": "./dist/api/client.js"
		},
		"./health": {
			"types": "./dist/api/health.d.ts",
			"import": "./dist/api/health.js",
			"default": "./dist/api/health.js"
		},
		"./service": {
			"types": "./dist/api/service.d.ts",
			"import": "./dist/api/service.js",
			"default": "./dist/api/service.js"
		},
		"./daemon-events": {
			"types": "./dist/api/daemon-events.d.ts",
			"import": "./dist/api/daemon-events.js",
			"default": "./dist/api/daemon-events.js"
		}
	},
	"types": "dist/api/index.d.ts",
	"typesVersions": {
		"*": {
			"client": ["dist/api/client.d.ts"],
			"health": ["dist/api/health.d.ts"],
			"service": ["dist/api/service.d.ts"],
			"daemon-events": ["dist/api/daemon-events.d.ts"]
		}
	},
	"scripts": {
		"build": "tsc && bun build src/index.ts --outfile dist/sv.js --target bun",
		"dev": "bun run src/index.ts",
		"check": "tsc --noEmit",
		"prepublishOnly": "bun run build"
	},
	"files": [
		"dist",
		"bin"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/collaborator-ai/seedvault.git",
		"directory": "cli"
	},
	"dependencies": {
		"chokidar": "^4"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"typescript": "^5"
	}
}