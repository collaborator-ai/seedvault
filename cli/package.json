{
  "name": "@seedvault/cli",
  "version": "0.6.0",
  "type": "module",
  "bin": {
    "sv": "bin/sv.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    }
  },
  "types": "dist/api/index.d.ts",
  "scripts": {
    "build": "tsc && bun build src/index.ts --outfile dist/sv.js --target bun",
    "dev": "bun run src/index.ts",
    "check": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/collaborator-ai/seedvault.git",
    "directory": "cli"
  },
  "dependencies": {
    "@seedvault/sdk": "workspace:*",
    "chokidar": "^4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  }
}
